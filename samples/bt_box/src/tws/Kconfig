config TWS_APP
	bool
	prompt "Tws App Support"
	depends on MEDIA_PLAYER
	depends on BT_MANAGER
	select STREAM
	select RINGBUFF_STREAM
	default n
	depends on TWS
	help
	This option enable or disable tws app


