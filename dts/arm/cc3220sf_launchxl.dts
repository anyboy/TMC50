/dts-v1/;

#include <ti/cc32xx.dtsi>

/ {
	model = "TI CC3220SF LaunchXL";
	compatible = "ti,cc3220sf-launchxl", "ti,cc3220sf", "ti,cc32xx";

	aliases {
		uart_0 = &uart0;
		uart_1 = &uart1;
	};

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash1;
		zephyr,console = &uart0;
	};
};

&uart0 {
	status = "ok";
	current-speed = <115200>;
};
