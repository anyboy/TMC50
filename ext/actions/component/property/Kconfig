menuconfig PROPERTY
	bool
	prompt "Property"
	depends on ACTIONS_COMPONENT_FUNCTION
	default n
	help
	This option enables actions property manager.
	
config PROPERTY_CACHE
	bool
	prompt "property cache"
	depends on PROPERTY
	default n
	help
	This option enables actions property manager.





